<div class="create_form">
	<form action="/ibook/unwantedList" method="POST">
		<input type="text" name="title"> <label> title</label> <br/>
		<input type="text" name="author"> <label> author </label> <br/>
		<input type="number" name="year"><label> year</label><br/>
		<textarea name="description" rows="6" cols="40"></textarea><label>  description</label><br/>
		<input type="text" name="image"><label> URL</label><br/>
		<select name="category">
			<% @category.each do |category| %>
			 <option value="<%=category.id%>"><%=category[:name]%></option>
			<% end %>
		</select><label> genre</label><br/>
		<input type="submit"><br/>
	</form>
</div>